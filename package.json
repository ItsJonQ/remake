{
  "name": "remake-cli",
  "version": "0.0.6",
  "private": false,
  "description": "A simple generator from locally defined templates",
  "main": "dist/index.js",
  "bin": {
    "remake": "dist/remake-cli.js"
  },
  "scripts": {
    "build:js": "zero build",
    "build": "npm run build:js",
    "clean": "rm -rf dist",
    "precommit": "zero pre-commit",
    "lint": "zero lint",
    "release": "np",
    "test:remake": "npm run remake --prefix example",
    "test": "npm run remake --prefix example -- component --name=Hello",
    "version": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "author": "Jon Quach <hello@jonquach.com> (https://jonquach.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsjonq/remake-cli.git"
  },
  "bugs": {
    "url": "https://github.com/itsjonq/remake-cli/issues"
  },
  "keywords": [
    "remake",
    "remake-cli",
    "cli",
    "react-cli",
    "react",
    "template",
    "generator"
  ],
  "dependencies": {
    "commander": "2.19.0",
    "lodash.template": "4.4.0",
    "meow": "5.0.0",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "pkg-up": "2.0.0"
  },
  "devDependencies": {
    "@helpscout/zero": "1.0.3",
    "np": "3.1.0"
  }
}
